{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Provider": "MongoDB",
    "IsEnabled": true

  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Db": "Server=(localdb)\\mssqllocaldb;Database=Starglade;Integrated Security=true"

  },
  "MongoDB": {
    "ConnectionStrings": "mongodb://localhost:27017",
    "Db": "Starglade"
  },
  "Redis": {
    "Configuration": "localhost",
    "InstanceName": ""
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "Vhost": "/",
    "User": "guest",
    "Pass": "guest",
    "Exchange": "Startglade_Direct",
    "ExchangeType": "direct",
    "Queue": "Starglade_CategoryCreated",
    "RoutingKey": "CategoryCreatedKey"
  },

  "JWT": {
    "SecretKey": "SOME_RANDOM_KEY_DO_NOT_SHARE",
    "Issuer": "http://localhost:52775",
    "ExpireDays": 30
  }



}
